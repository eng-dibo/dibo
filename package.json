{
  "name": "@eng-dibo/dibo",
  "description": "A monorepo for multiple projects",
  "version": "0.0.0-development",
  "workspaces": [
    "./projects/*",
    "./packages/*"
  ],
  "scripts": {
    "setup": "npm cache clean -f &&  npm i -D husky && npm i --unsafe-perm --production=false --include=dev -f -ws",
    "prepare": "husky install",
    "postinstall": "ngcc",
    "lint": "prettier --write . && eslint --fix --config .eslintrc.js --ext .js,.ts",
    "test": "jest",
    "test:all": "jest --coverage --only-changed=false",
    "semantic-release": "semantic-release",
    "vscode": "export project=$PWD && npm run docker:stop --prefix=projects/vscode && npm run docker:run --prefix=projects/vscode",
    "task": "ts-node  -r tsconfig-paths/register ./tasks/index.ts",
    "tasks:build": "webpack --entry=./tasks/index.ts  --output-path=./tasks/tasks~~.js"
  },
  "lint-staged": {
    "*": "npm run lint"
  },
  "private": false,
  "globalDependencies": [
    "webpack-cli@^4.8.0",
    "ts-node@10.2.1"
  ],
  "dependencies": {
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@types/node": "^15.3.1",
    "@types/webpack": "^5.28.0",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/eslint-plugin-tslint": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "codelyzer": "^6.0.0",
    "eslint": "^7.25.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^33.0.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "husky": "^6.0.0",
    "lint-staged": "^10.5.4",
    "prettier": "^2.4.1",
    "semantic-release": "^17.4.4",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.2.1",
    "tsconfig-paths": "^3.11.0",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "tslint": "~6.1.0",
    "typescript": "^4.3.5",
    "webpack": "^5.54.0",
    "webpack-cli": "^4.8.0",
    "webpack-merge": "^5.8.0"
  },
  "keywords": [
    "nodejs",
    "front-end",
    "back-end",
    "database",
    "angular",
    "react",
    "vue"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/eng-dibo/dibo.git"
  },
  "homepage": "https://github.com/eng-dibo/dibo#readme",
  "bugs": {
    "url": "https://github.com/eng-dibo/dibo/issues",
    "email": "sh.eldeeb.2010+github@gmail.com"
  },
