    "cross-env": "7.0.3",
    "dotenv": "11.0.0",
    "ejs": "3.1.6",
    "jasmine-core": "4.0.1",
    "jasmine-spec-reporter": "7.0.0",
    "jest-preset-angular": "11.1.1",
    "nodemon": "2.0.15",
    "protractor": "7.0.0",
    "semantic-release": "19.0.2",
    "semantic-release-monorepo": "7.0.5",
    "supertest": "6.2.2",
