    "quill": "^1.3.7",
    "sharp": "^0.28.3",
    "shortid": "^2.2.16",
    "express": "^4.17.1",
    "@google-cloud/storage": "^5.14.8",
    "@nguniversal/express-engine": "^12.1.0",
    "deasync": "^0.1.23",
    "strip-json-comments": "^3.1.1",
    "ts-jest": "^27.0.5",
