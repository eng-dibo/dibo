{
  "name": "@eng-dibo/dibo",
  "description": "A monorepo for multiple projects",
  "version": "0.0.0-development",
  "workspaces": [
    "./projects/*",
    "./packages/*"
  ],
  "scripts": {
    "prepare": "husky install",
    "lint": "prettier . --write --ignore-path=.gitignore --ignore-unknown && eslint . --fix --config=.eslintrc.js --ext=.js,.ts --ignore-path=.gitignore",
    "test": "jest --coverage --only-changed=false",
    "test:fails": "jest --coverage --only-changed=false --only-failures",
    "test:changed": "jest",
    "release": "semantic-release",
    "release:local": "semantic-release --no-ci",
    "vscode": "export project=$PWD && npm run docker:stop --prefix=projects/vscode && npm run docker:run --prefix=projects/vscode",
    "task": "ts-node  -r tsconfig-paths/register ./tasks/index.ts",
    "tasks:build": "webpack --entry=./tasks/index.ts  --output-path=./tasks/tasks~~.js",
    "build:packages": "npm run build -w packages",
    "publish:packages": "npm run _publish -w packages",
    "generate": "npm run task -- generate",
    "link": "npm run task -- link",
    "delete-tags": " npm run delete-tags:remote && npm run delete-tags:local",
    "delete-tags:local": "git tag -d $(git tag -l)",
    "delete-tags:remote": "git fetch && git push origin --delete $(git tag -l)",
    "update:all": "npm-check-updates -u --deep --enginesNode --peer  --target=minor && npm i && npm run test",
    "update:all:major": "npm-check-updates -u --deep --enginesNode --peer  --target=major && npm i && npm run test"
  },
  "lint-staged": {
    "*": [
      "prettier --write --ignore-path=.gitignore --ignore-unknown",
      "eslint --fix --config=.eslintrc.js --ext=.js,.ts --ignore-path=.gitignore"
    ]
  },
  "private": false,
  "dependencies": {
    "jest": "28.1.0",
    "jest-preset-angular": "^11.1.2",
    "rxjs": "^7.5.5",
    "strip-json-comments": "^3.1.1",
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "@angular-eslint/eslint-plugin": "^13.2.1",
    "@angular-eslint/eslint-plugin-template": "^13.2.1",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^9.0.1",
    "@types/ejs": "^3.1.0",
    "@types/node": "^17.0.30",
    "@types/webpack": "^5.28.0",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/eslint-plugin-tslint": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "codelyzer": "^6.0.2",
    "ejs": "^3.1.7",
    "eslint": "^8.15.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^37.9.7",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "husky": "^8.0.1",
    "lint-staged": "^12.4.1",
    "npm-check-updates": "^12.5.10",
    "prettier": "^2.6.2",
    "semantic-release": "^19.0.2",
    "semantic-release-monorepo": "^7.0.5",
    "shx": "^0.3.4",
    "ts-jest": "28.0.2",
    "ts-loader": "^9.2.9",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^3.14.1",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "^4.6",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2",
    "webpack-merge": "^5.8.0"
  },
  "keywords": [
    "nodejs",
    "front-end",
    "back-end",
    "database",
    "angular",
    "react",
    "vue"
  ],
  "repository": {
    "type": "git",
