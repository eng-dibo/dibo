{
  "name": "ngx-cms",
  "version": "0.0.1",
  "scripts": {
    "start": "npm run build && npm run serve",
    "start:dev": "npm run build:dev && npm run serve:dev",
    "start:deploy": "npm run build && npm run deploy",
    "deploy": "npm run task -- deploy",
    "task": "ts-node  -r tsconfig-paths/register ./tasks/index.ts",
    "build": "npm run task -- build",
    "build:dev": "npm run task -- build --mode=development",
    "build:browser:dev": "npm run task -- build --mode=development --targets=browser",
    "build:server:dev": "npm run task -- build --mode=development --targets=server",
    "serve": "ts-node -r dotenv/config  ../../dist/ngx-cms/core/server/main.js --start dotenv_config_path=../../dist/ngx-cms/config/server/.env -- --inspect",
    "serve:dev": "cross-env NODE_ENV=development npm run serve",
    "serve:ng": "ng serve",
    "prerender": "ng run ngx-cms:prerender",
    "setup": "npm run task -- setup",
    "postinstall": "ngcc",
    "kill-port:linux": "fuser -k 4200/tcp"
  },
  "private": false,
  "main": "../../dist/ngx-cms/core/server/express.js",
  "dependencies": {
    "@angular/animations": "^13.1.2",
