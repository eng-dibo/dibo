{
  "name": "ngx-cms",
  "version": "0.0.1",
  "scripts": {
    "postinstall": "ngcc",
    "start": "npm run build && npm run serve",
    "start:dev": "npm run build:dev && npm run serve:inspect",
    "start:nodemon": "nodemon  . -e ts,html,scss --exec \"npm run start:dev\"",
    "ng:local": "npx -p @angular/cli ng",
    "ng": "ng",
    "build:browser": "ng build --aot --prod",
    "build:browser:dev": "ng build",
    "build:server": "ng run ngx-cms:server:production --bundle-dependencies",
    "build:server:dev": "ng run ngx-cms:server --bundle-dependencies",
    "build:config": "webpack -c config/webpack.config.ts",
    "build": "cross-env-shell NODE_ENV=production \"npm run build:browser && npm run build:server && npm run build:config\"",
    "build:dev": "cross-env-shell NODE_ENV=development \"npm run build:browser:dev && npm run build:server:dev && npm run build:config\"",
    "serve": "node -r dotenv/config ../../dist/ngx-cms/core/server/main.js --start dotenv_config_path=../../dist/ngx-cms/config/.env",
    "serve:ng": "ng serve -o",
    "serve:ng:ssr": "ng run ngx-cms:serve-ssr",
    "serve:inspect": "npm run serve --node-options --inspect",
    "serve:watch": "nodemon ../../dist/ngx-cms/core/server/main.js --start",
    "update": "npm run update --prefix=../.. && ng update --force --allow-dirty",
    "prerender": "ng run ngx-cms:prerender",
    "docker:build": "npx ncp Dockerfile ../../dist/Dockerfile && npx docker build ../../dist -t gcr.io/dibo-cloud/ngx-cms",
    "docker:view": "docker run -it --rm=true gcr.io/dibo-cloud/ngx-cms sh",
    "docker:push": "docker push gcr.io/dibo-cloud/ngx-cms",
    "docker:run": "docker run -it -p 4200:4200 --rm gcr.io/dibo-cloud/ngx-cms",
    "gcloud:start": "npm run gcloud:login && npm run docker:build && npm run docker:push && npm run gcloud:deploy",
    "gcloud:login": "gcloud auth login",
    "gcloud:deploy": "gcloud run deploy cms-run --image=gcr.io/dibo-cloud/ngx-cms --platform=managed --port=4200 --region=europe-west1 --allow-unauthenticated",
    "gcloud:run": "PORT=4200 && docker run -p 9090:${PORT} -e PORT=${PORT} --rm gcr.io/dibo-cloud/ngx-cms"
  },
  "private": false,
  "main": "../../dist/ngx-cms/core/server/express.js",
  "dependencies": {
    "@angular/material": "^12.2.6",
    "@angular/animations": "^12.2.6",
    "@angular/common": "^12.2.6",
    "@angular/compiler": "^12.2.6",
    "@angular/core": "^12.2.6",
    "@angular/forms": "^12.2.6",
