{
  "name": "@eng-dibo/dibo",
  "description": "A monorepo for multiple projects",
  "version": "0.0.0-development",
  "scripts": {
    "lint": "prettier --write . && eslint --fix --config .eslintrc.js --ext .js,.ts",
    "prepare": "husky install",
    "test": "jest",
    "test:all": "jest --coverage --only-changed=false",
    "semantic-release": "semantic-release",
    "vscode": "export project=$PWD && npm run docker:stop --prefix=projects/vscode && npm run docker:run --prefix=projects/vscode",
    "task": "ts-node  -r tsconfig-paths/register ./tasks/index.ts",
    "tasks:build": "webpack --entry=./tasks/index.ts  --output-path=./tasks/tasks~~.js",
    "ng:update": "ng update --force --allow-dirty",
    "update": "npm run ng:update @angular/core@latest @angular/cli@latest && npm run ng:update @angular/material && npm run ng:update"
  },
  "lint-staged": {
    "*": "npm run lint"
  },
  "engines": {
    "node": "12",
    "npm": "7"
  },
  "os": [
    "linux"
  ],
  "cpu": [
    "x64"
  ],
  "private": false,
  "globalDependencies": [
    "@angular/cli@11.2.11"
  ],
  "dependencies": {
