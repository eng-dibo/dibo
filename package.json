{
  "name": "@eng-dibo/dibo",
  "description": "A monorepo for multiple projects",
  "version": "0.0.0-development",
  "workspaces": [
    "./projects/*",
    "./packages/*"
  ],
  "scripts": {
    "prepare": "husky install",
    "lint": "npm run lint:prettier && npm run lint:eslint",
    "lint:prettier": "prettier . --write --ignore-path=.gitignore --ignore-unknown",
    "lint:eslint": "eslint \"**\" --fix --config=.eslintrc.js --ext=.* --ignore-path=.gitignore --quiet",
    "test": "jest --coverage --only-changed=false",
    "test:fails": "jest --coverage --only-changed=false --only-failures",
    "test:changed": "jest",
    "release": "semantic-release",
    "release:local": "semantic-release --no-ci",
    "vscode": "export project=$PWD && npm run docker:stop --prefix=projects/vscode && npm run docker:run --prefix=projects/vscode",
    "task": "ts-node  -r tsconfig-paths/register ./tasks/index.ts",
    "tasks:build": "webpack --entry=./tasks/index.ts  --output-path=./tasks/tasks~~.js",
    "build:packages": "npm run build -w packages",
    "publish:packages": "npm run _publish -w packages",
    "generate": "npm run task -- generate",
    "link": "npm run task -- link",
    "delete-tags": " npm run delete-tags:remote && npm run delete-tags:local",
    "delete-tags:local": "git tag -d $(git tag -l)",
    "delete-tags:remote": "git fetch && git push origin --delete $(git tag -l)",
    "update:all": "npm-check-updates && npm i && npm run test",
    "update:all:major": "npm-check-updates  --target=major && npm i && npm run test"
  },
  "lint-staged": {
    "*": [
      "prettier --write --ignore-path=.gitignore --ignore-unknown",
      "eslint --fix --config=.eslintrc.js --ignore-path=.gitignore --ext=.*"
    ]
  },
  "private": false,
  "devDependencies": {
    "@angular-eslint/eslint-plugin": "^13.2.1",
    "@angular-eslint/eslint-plugin-template": "^13.2.1",
    "@angular-eslint/template-parser": "^13.2.1",
