    "@types/deasync": "^0.1.2",
    "@types/jsdom": "^16.2.14",
    "@types/multer": "^1.4.7",
    "@types/ncp": "^2.0.5",
    "@types/node": "^17.0.8",
    "@types/quill": "^2.0.9",
    "@types/rss": "^0.0.29",
    "@types/supertest": "^2.0.11",
    "@types/web-push": "^3.3.2",
    "cross-env": "^7.0.3",
    "dotenv": "^11.0.0",
    "ejs": "^3.1.6",
    "jasmine-core": "~4.0.0",
    "jasmine-spec-reporter": "~7.0.0",
    "jest-preset-angular": "^11.0.1",
    "nodemon": "^2.0.15",
    "protractor": "~7.0.0",
    "semantic-release": "^19.0.2",
    "semantic-release-monorepo": "^7.0.5",
    "supertest": "^6.2.1",
    "terser": "^5.12.0",
