{
  "name": "ngx-cms",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "build:browser": "ng build --aot --prod",
    "build:browser:dev": "ng build",
    "build:server": "ng run ngx-cms:server:production --bundleDependencies",
    "build:server:dev": "ng run ngx-cms:server --bundleDependencies",
    "build:all": "npm run build:browser && npm run build:server",
    "build:all:dev": "npm run build:browser:dev && npm run build:server:dev",
    "build": "npx cross-env NODE_ENV=production npm run build:all",
    "build:dev": "npx cross-env NODE_ENV=development npm run build:all:dev",
    "serve": "node ../../dist/ngx-cms/core/server/main.js --start",
    "serve:ng": "ng serve -o",
    "serve:inspect": "node ../../dist/ngx-cms/core/server/main.js --start",
    "serve:dev": "nodemon ../../dist/ngx-cms/core/server/main.js --start",
    "serve:ssr:dev": "ng run ngx-cms:serve-ssr",
    "start": "npm run build && npm run serve",
    "start:dev": "npm run build:dev && npm run serve:inspect",
    "start:nodemon": "nodemon  . -e ts,html,scss --exec \"npm run start:dev\"",
    "update": "npm run update --prefix=../.. && ng update --force --allow-dirty",
    "prerender": "ng run ngx-cms:prerender"
  },
  "private": false,
  "main": "../../dist/ngx-cms/core/server/main.js",
  "dependencies": {
    "@angular/material": "^12.2.6"
  },
  "devDependencies": {},
  "repository": {
    "type": "git",
    "url": "https://github.com/eng-dibo/dibo.git"
  },
  "homepage": "https://github.com/eng-dibo/dibo#readme",
  "bugs": {
    "url": "https://github.com/eng-dibo/dibo/issues",
    "email": "sh.eldeeb.2010+github@gmail.com"
  },
  "license": "MIT",
  "author": "Sherif Eldeeb <sh.eldeeb.2010+github@gmail.com> (https://github.com/eng-dibo)",
  "funding": [
